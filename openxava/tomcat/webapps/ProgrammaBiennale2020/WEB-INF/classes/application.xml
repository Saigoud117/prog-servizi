<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE application SYSTEM "dtds/application.dtd"><application name="ProgrammaBiennale2020"> 		<!-- 	A default module for each entity is assumed with the	controllers on <default-module/>	-->	<default-module>		  		<!-- <controller name="Typical"/> -->			<controller name="ProgrammaBiennale"/>	</default-module>		<module name="GestioneReferenti">	    <model name="Inquadrato"/>	    <tab name="Referenti"/>	    <controller name="GestioneReferenti"/>	</module>		<module name="ArchivioRedazioneProcedure">	    <model name="Procedure"/>	    <tab name="Archived"/> <!-- To show only the deleted entities -->	</module>		<module name="ArchivioModificheProcedure">	    <model name="ProcedureProgramma"/>	    <tab name="Archived"/> <!-- To show only the deleted entities -->	</module>		<module name="ArchivioProgrammaAcquisti">	    <model name="ProcedureDefinitive"/>	    <tab name="Archived"/> <!-- To show only the deleted entities -->	</module>	    <module name="CestinoProcedure">	    <env-var name="XAVA_LIST_ACTION"	        value="Trash.restore"/> <!-- The action to be shown in each row -->	    <model name="Procedure"/>	    <tab name="Deleted"/> <!-- To show only the deleted entities -->	    <controller name="Trash"/> <!-- With only one action: restore -->	    <mode-controller name="ListOnly"/> <!-- Only list mode -->	</module>		<module name="CestinoProcedureProgramma">	    <env-var name="XAVA_LIST_ACTION"	        value="Trash.restore"/> <!-- The action to be shown in each row -->	    <model name="ProcedureProgramma"/>	    <tab name="Deleted"/> <!-- To show only the deleted entities -->	    <controller name="Trash"/> <!-- With only one action: restore -->	    <mode-controller name="ListOnly"/> <!-- Only list mode -->	</module>		<module name="CestinoProcedureDefinitive">	    <env-var name="XAVA_LIST_ACTION"	        value="Trash.restore"/> <!-- The action to be shown in each row -->	    <model name="ProcedureDefinitive"/>	    <tab name="Deleted"/> <!-- To show only the deleted entities -->	    <controller name="Trash"/> <!-- With only one action: restore -->	    <mode-controller name="ListOnly"/> <!-- Only list mode -->	</module>		<module name="Dashboard">		<model name="Dashboard" />		<controller name="Dashboard"/>	</module>		<!-- 	You can define additional modules, for example:	  	<module name="OnlyReadTeacher">		<model name="Teacher"/>		<controller name="Print"/>	</module>			-->	</application>